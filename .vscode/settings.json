// Place your settings in this file to overwrite default and user settings.
{
    // Exclude js.map files and js files when doing typescript (same file exists as a .ts file) 
	"files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
        "**/*.js.map": true,
        "**/*.js": {"when": "$(basename).ts"}
	}
}